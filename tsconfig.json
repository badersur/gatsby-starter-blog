{
    "compilerOptions": {
        "target": "ESNext",
        "module": "CommonJS",
        "allowJs": true,
        "checkJs": true,
        "noEmit": true,
        "jsx": "react",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "strict": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true
    },
    "exclude": ["node_modules", "public", ".cache"]
}

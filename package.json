{
    "name": "gatsby-starter-blog",
    "private": true,
    "description": "A starter for a blog powered by Gatsby, TypeScript, ESLint and Markdown",
    "version": "1.1.2",
    "author": "Bader Nasser <badersur@gmail.com>",
    "scripts": {
        "type-check": "tsc --noEmit",
        "lint:prettier": "prettier -c '**/*.{html,json,yml,yaml,css,md}'",
        "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint": "run-p lint:*",
        "fmt:prettier": "run-s \"lint:prettier -- --write\"",
        "fmt:eslint": "run-s \"lint:eslint -- --fix\"",
        "fmt": "run-p fmt:*",
        "start": "run-s develop",
        "clean": "gatsby clean",
        "develop": "gatsby develop",
        "build": "gatsby build",
        "serve": "gatsby serve",
        "serve:prod": "run-s clean build serve",
        "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
    },
    "bugs": {
        "url": "https://github.com/badersur/gatsby-starter-blog/issues"
    },
    "dependencies": {
        "gatsby": "^2.24.62",
        "gatsby-image": "^2.4.19",
        "gatsby-plugin-feed": "^2.5.12",
        "gatsby-plugin-google-analytics": "^2.3.14",
        "gatsby-plugin-manifest": "^2.4.30",
        "gatsby-plugin-offline": "^3.2.28",
        "gatsby-plugin-react-helmet": "^3.3.11",
        "gatsby-plugin-sharp": "^2.6.36",
        "gatsby-plugin-typescript": "^2.4.20",
        "gatsby-plugin-typography": "^2.5.11",
        "gatsby-remark-copy-linked-files": "^2.3.15",
        "gatsby-remark-images": "^3.3.30",
        "gatsby-remark-prismjs": "^3.5.13",
        "gatsby-remark-responsive-iframe": "^2.4.14",
        "gatsby-remark-smartypants": "^2.3.11",
        "gatsby-source-filesystem": "^2.3.30",
        "gatsby-transformer-remark": "^2.8.35",
        "gatsby-transformer-sharp": "^2.5.15",
        "prismjs": "^1.21.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-helmet": "^6.1.0",
        "react-typography": "^0.16.19",
        "typeface-merriweather": "0.0.72",
        "typeface-montserrat": "0.0.75",
        "typography": "^0.16.19",
        "typography-theme-wordpress-2016": "^0.16.19"
    },
    "devDependencies": {
        "@types/node": "^14.10.2",
        "@types/react": "^16.9.49",
        "@types/react-dom": "^16.9.8",
        "@types/react-helmet": "^6.1.0",
        "@types/typography": "^0.16.3",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
        "@typescript-eslint/parser": "^2.34.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.20.6",
        "eslint-plugin-react-hooks": "^1.7.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.1.2",
        "ts-node": "^9.0.0",
        "typescript": "^4.0.2",
        "vfile-message": "^2.0.4"
    },
    "homepage": "https://github.com/badersur/gatsby-starter-blog#readme",
    "keywords": [
        "gatsby"
    ],
    "license": "MIT",
    "main": "n/a",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/badersur/gatsby-starter-blog.git"
    }
}
